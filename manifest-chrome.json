{
    "manifest_version": 2,
    "name": "Questrade Real Time Quote",
    "version": "1.2",
    "description": "Questrade Real Time Quote automatically clicks the refresh quote in Questrade every second.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://my.questrade.com/*"
            ],
            "js": [
                "background.js"
            ]
        }
    ]
}
